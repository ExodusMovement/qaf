{
  "license": "MIT",
  "name": "qaf",
  "description": "Simple state management for React based on context.",
  "version": "0.0.1",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sonaye/qaf.git"
  },
  "peerDependencies": {
    "react": ">=16.3.0"
  },
  "devDependencies": {
    "babel-eslint": "8.2.3",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.9.1",
    "gh-pages": "1.2.0",
    "jest": "23.1.0",
    "page": "1.8.6",
    "pagify-it": "0.0.8",
    "parcel-bundler": "1.9.2",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-test-renderer": "16.4.1",
    "rollup": "0.61.1",
    "rollup-plugin-babel": "3.0.4"
  },
  "scripts": {
    "prestart": "rm -rf .cache dev",
    "start": "parcel examples/index.html --out-dir dev --open",
    "lint": "eslint src test examples",
    "test": "jest test",
    "coverage": "yarn test --coverage",
    "postcoverage": "open -a Chrome coverage/lcov-report/index.html",
    "prebuild": "rm -rf lib",
    "build": "rollup -c",
    "preexamples": "rm -rf build",
    "examples": "parcel build examples/index.html --out-dir build --public-url /qaf/",
    "predeploy": "yarn examples",
    "deploy": "gh-pages -d build"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "verbose": true
  },
  "files": [
    "lib"
  ]
}
