{
  "env": {
    "browser": true,
    "jest": true
  },
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "arrow-parens": 0,
    "comma-dangle": [2, "never"],
    "function-paren-newline": 0,
    "no-confusing-arrow": 0,
    "object-curly-newline": 0,
    "react/jsx-filename-extension": 0,
    "react/prop-types": 0
  }
}
